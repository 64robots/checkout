{
    "name": "64robots/checkout",
    "description": "Checkout Package.",
    "keywords": ["framework", "laravel", "checkout"],
    "license": "MIT",
    "type": "library",
    "require": {
        "laravel/framework": "^6.0",
        "php": "^7.1.3",
        "illuminate/support": "^6.10"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "files": [
            "src/Helpers/Globals.php"
        ],
        "psr-4": {
            "R64\\Checkout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "R64\\Checkout\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "R64\\Checkout\\CheckoutServiceProvider",
                "R64\\Checkout\\CheckoutAuthServiceProvider"
            ]
        }
    }
}
